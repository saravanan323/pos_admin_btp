<core:FragmentDefinition xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <ActionSheet id="actionSheet"
        xmlns="sap.m"
        xmlns:core="sap.ui.core" core:require="{ MessageToast: 'sap/m/MessageToast' }" showCancelButton="true" placement="Bottom">
        <Button text="{i18n>dhbtnSettings}" icon="sap-icon://action-settings" press="onPressUserSetting" />
        <Button text="{i18n>dhbtnLogout}" icon="sap-icon://log" press="onPressLogOut" />
    </ActionSheet>
</core:FragmentDefinition>
